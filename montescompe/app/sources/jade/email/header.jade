div(class="header text-center" align="center")
	table(class="container"): tbody: tr: td
		table(class="row collapse"): tbody: tr
			th(class="small-6 large-6 columns first")
				if logo
					table: tr: th: img(src="#{pub_img}#{logo}",height="#{logo_height}",style="height:#{logo_height} !important;")
			th(class="small-6 large-6 columns last",style="vertical-align:middle;")
				if name_right
					table: tr: th: p(class="text-right") #{name_right}