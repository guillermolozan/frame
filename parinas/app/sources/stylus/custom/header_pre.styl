.header_pre
	nav
		background color2 url(imgs+'header-bg2.png') repeat-y center top !important
		color white
		.container
			height 42px
			background color2 url(imgs+'header-bg1.png?3') no-repeat right top
			li 
				font-size 17px
				// width 25px
				height 25px
				a
					width 100%
					height 100%
					background-repeat no-repeat
					&:before
						content "" !important
			li.facebook a
				background-image url(imgs+'header-facebook.png')
			li.youtube a
				background-image url(imgs+'header-youtube.png')
			li.twitter a
				background-image url(imgs+'header-twitter.png')
			li.phone,li.email
				background-repeat no-repeat
				background-position center left
				&:before
					content "" !important
			li.email
				padding-left 25px !important
				background-image url(imgs+'header-mail.png')
				margin-right 50px
			li.phone
				// width 340px
				order 100 !important
				padding-left 20px !important
				background-image url(imgs+'header-phone.png')