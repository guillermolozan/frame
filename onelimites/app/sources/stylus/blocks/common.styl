//font

html 
	font-family Roboto,'Open Sans', sans-serif;
	// font-size 1.05em
	@media $small
		font-family Roboto, sans-serif
		font-size 1.05em
		
.preheader,
.p-home .line_a,
.p-home .servicios
footer,
.footerpre2
	font-family Roboto,Dosis,Roboto, sans-serif
h1,h2,h3
	font-family Orbitron !important
	text-transform uppercase !important
	color black !important
	letter-spacing -0.5px
h3
	font-size 15px !important
	font-weight bold
p
	color #999 !important

.p-home
	h2 
		font-size 28px !important

	
.menu_top > li > a
	font-family Montserrat,Roboto,Neucha, sans-serif !important
	font-size 1.1em !important
.post
	font-family Roboto,Dosis, sans-serif
	






.collection
	.collection-item
		padding 7px 5px
		a
			display block
			font-weight 400
			font-size 1em
			// color color5
		&.active
			background-color lighten(black,40)
			color white
			a
				color inherit
			

.footerpre2
	background-color #000
	// margin-top -20px !important
	color pre_footer_textcolor
	.container
		margin-bottom 0 !important
		@media $small
			padding-left 0
			padding-right 0
		> .col
			padding-top 3em
			position relative
			p
				color inherit !important
				font-weight 300
				@media $small-medium
					br 
						content " "
						&:after
							content " "
				strong
					display block
					margin-top .8em
					&.title
						margin-bottom .7em
						margin-top 1em
			&.puntos:after
				content ""
				width 2px
				height 80%
				border-left 2px dashed #fff
				border-right 2px dashed #fff
				position absolute
				top 1.4em
				right 0
				display none
				@media $small-medium
					border 0
			&.negro
				background #000
				padding-bottom 20px
				padding-top 15px
				padding-left 20px
				a, span
					display block
					margin-bottom 0
					color #fff
					padding-left 40px
					@media $small
						padding-left 22px
					strong 
						margin 0
					background-repeat no-repeat
					background-position 0 0
					&.ventas
						background-image url(imgs+'cart.png')
						background-size 15%
						height 50px
						background-position -3px 0
						@media $small-medium
							background-size 5%
						@media $small
							background-size 7%
					&.facebook
						background-image url(imgs+'face.png')
						background-size 12%
						height 40px
						@media $small-medium
							background-size 4%
						@media $small
							background-size 5%
					&.email
						background-image url(imgs+'mail.png')
						background-size 12%
						height 40px
						padding-top 10px
						@media $small-medium
							background-size 4%
						@media $small
							background-size 6%
							padding-top 0px
	strong 
		color pre_footer_textcolor
		font-weight 700 !important
			

footer
