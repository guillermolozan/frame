ul.menu_center
	padding-left 2em
	padding-right 2em
	display flex
	justify-content center
	margin-bottom 0
	padding-bottom 2em
	text-align center
	@media $small
		display block
		padding-left 0
		padding-right 0
		height auto
		text-align center
	> li
		background transparent
		flex-grow 1
		flex-shrink 1
		flex-basis auto
		text-align center
		position relative
		float none
		@media $small
			display inline-block
			// float left
	>li.proteger,
	>li.entrenar,
	>li.rehabilitar,
	>li.reducir,
	>li.divertir
		a
			// text-align: center
			padding-top 100px
			width auto
			height 91px
			display inline-block
			color #000
			font-family Bitter !important
			font-size 1.2em
			font-weight 700
			text-transform uppercase
			background-repeat no-repeat
			background-position center 0
			background-size 91px
			min-width 91px
			// zoom 100%
			transition height .5s,background-size .5s,padding-top .5s,font-size .5s
			
			@media $small
				background-size 55px
				padding-top 59px
				font-size .8em !important
				padding-left 0 !important
				padding-right 0 !important
				height 55px
				background-size 25px
				padding-top 25px
				// background-color red

			
		&.entrenar a
			background-image url(imgs+'logo_entrenar.png')
		&.proteger a
			background-image url(imgs+'logo_proteger.png')
		&.reducir a
			background-image url(imgs+'logo_reducir.png')
		&.divertir a
			background-image url(imgs+'logo_divertir.png')
		&.rehabilitar a
			background-image url(imgs+'logo_rehabilitar.png')
	&.menu_top
