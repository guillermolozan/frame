if(gallery.items)

	ul(class="col #{col_ul} #{ul_class}")

		each item in gallery.items

			li(class="col #{col_li}")

				div(class="card")

					h3: +item(item)
					//- div(class="card-content col s12")
					div(class="card-image")
						//- +img_magnific(item)
						//- +img_item(item)	
						a(href="#{item.url}"): img(src="#{item.img}",title="#{item.name}",alt="#{item.name}",class="responsive-img")
					div(class="card-content")
						.row
							div(class="col s12 m6")
								h5.operacion #{item.tipo}
							div(class="col s12 m6")
								h5.precio #{item.precio}
						.row
							div(class="col s12 m6")
								.tipo #{item.operacion}
							div(class="col s12 m6 contactar")
								a.btn(href="#{item.url}#cotizar") contactar
						//- | #[strong Operacion]: #{item.operacion}