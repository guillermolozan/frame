section.home_block_notes.row
	.container
		if(noticias)
			.col.s12.l4.noticias
				+list_card(noticias)

		if(brochure)
			.col.s12.l4.brochure
				.card
					.card-content
						.card-title
							h3 Brochure corporativo
						img(src="#{pub_img}#{brochure.img}")
						div: strong.descargar: a(target="_blank",href="#{brochure.file}") Descargar

		if(youtube)
			.col.s12.l4.youtube
				.card
					.card-content
						.card-title
							h3 #{youtube.name}
						.video-container
							iframe(width="853",height="480",src="//www.youtube.com/embed/#{youtube.code}?rel=0",frameborder="0",allowfullscreen="allowfullscreen")
		
		if(social)
			.col.s12.l4.social
				.card
					.card-content
						.card-title
							h3 Social
						div(
							class="fb-page",
							data-href="#{social}",
							data-tabs="timeline",
							data-small-header="false",
							data-adapt-container-width="true",
							data-hide-cover="false",
							data-show-facepile="true")
